# Hakilens API
VITE_HAKILENS_API_URL=https://YOUR-HAKILENS-API-URL

# LLM Provider (OpenRouter / OpenAI / etc.)
VITE_LLM_PROVIDER=openrouter
VITE_LLM_API_KEY=YOUR_LLM_API_KEY
